{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended"
    ],
    "rangeStrategy": "bump",
    "ignorePaths": [
        "frontend/android/",
        "frontend/macos/",
        "frontend/ios/"
    ],
    "prHourlyLimit": 0
    "lockFileMaintenance": { "enabled": true }
}
